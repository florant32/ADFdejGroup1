{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdejaprilgrp1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobdejgroup1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobdejgroup1'"
		},
		"LK_storagedejgroup1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LK_storagedejgroup1'"
		},
		"LS_storageaprilgrp1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_storageaprilgrp1'"
		},
		"synapsedej123-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapsedej123-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:synapsedej123.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"LS_storageaprilgrp1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedejaprilgrp1.dfs.core.windows.net/"
		},
		"synapsedej123-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://felixstorage1234.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdejaprilgrp1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobdejgroup1_connectionString": {
						"value": "[parameters('AzureBlobdejgroup1_connectionString')]"
					},
					"LS_storageaprilgrp1_accountKey": {
						"value": "[parameters('LS_storageaprilgrp1_accountKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"LK_storagedejgroup1_connectionString": {
						"value": "[parameters('LK_storagedejgroup1_connectionString')]"
					},
					"synapsedej123-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('synapsedej123-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"LS_storageaprilgrp1_properties_typeProperties_url": {
						"value": "[parameters('LS_storageaprilgrp1_properties_typeProperties_url')]"
					},
					"synapsedej123-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('synapsedej123-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}